{
  "apps": [
    {
      "root": "Client",
      "outDir": "wwwroot",
      "main": "main.browser.ts",
      "assets": [
        "assets/**/*"
      ],
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "tsconfig": "tsconfig.json",
      "faviconConfig": "favicon-config.json",
      "dlls": [
        {
          "entry": "polyfills.ts",
          "importToMain": true
        },
        {
          "entry": "vendors.ts"
        }
      ],
      "publicPath": "/",
      "index": "",
      "htmlInjectOptions": {
        "indexOutFileName": "../Views/Shared/_BundledScripts.cshtml",
        "iconsOutFileName": "../Views/Shared/_FavIcons.cshtml",
        "stylesOutFileName": "../Views/Shared/_BundledStyles.cshtml",
        "customTagAttributes": [
          {
            "tagName": "link",
            "attribute": {
              "asp-append-version": true
            }
          },
          {
            "tagName": "script",
            "attribute": {
              "asp-append-version": true
            }
          }
        ]
      },
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "buildTargetOverrides": {
        "dev": {},
        "prod": {
          "moduleReplacements": [],
          "compressAssets": true,
          "sourceMap": false
        }
      }
    }
  ]
}