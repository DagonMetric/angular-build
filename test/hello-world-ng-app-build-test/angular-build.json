{
  "apps": [
    {
      "srcDir": "src",
      "outDir": "dist",
      "entry": "main.ts",

      "copy": [ "favicon.ico" ],
      "publicPath": "/",
      "htmlInject": {
        "index": "index.html"
      },

      "envOverrides": {
        "prod": {
          "moduleReplacements": [
            {
              "resourcePath": "./environments/environment.ts",
              "newResourcePath": "./environments/environment.prod.ts"
            }
          ]
        }
      }
    }
  ]
}
