{
  //"masterPicture": "logo.png", 'favicon.svg
  "iconsPath": "icons-[hash]/",
  "design": {
    //"favicons": true,
    "desktopBrowser": {},

    // android
    "androidChrome": {
      "pictureAspect": "noChange",
      //"themeColor": "#ffffff",
      "manifest": {
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": false
      }
    },

    // appleIcon
    // appleStartup
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      //"backgroundColor": "#ffffff",
      "margin": "0", // 14%
      "assets": {
        "ios6AndPriorIcons": false,
        "ios7AndLaterIcons": true,
        "precomposedIcons": false,
        "declareOnlyDefaultIcon": true
      },
      // appleStartup
      "startupImage": {
        //"backgroundColor": "#FFFFFF"
      }
    },

    "safariPinnedTab": {
      "pictureAspect": "silhouette"
      //"themeColor": "#5bbad5"
    },

    "windows": {
      "pictureAspect": "noChange",
      //"backgroundColor": "#da532c",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": false,
        "windows10Ie11EdgeTiles": {
          "small": false,
          "medium": true,
          "big": false,
          "rectangle": false
        }
      }
    },

    //"firefox": true,
    "firefoxApp": {
      "pictureAspect": "circle",
      "keepPictureInCircle": true,
      "circleInnerMargin": "0",
      "margin": "0",
      //"backgroundColor": "#FFFFFF"
      "manifest": {
        //"appName": null,
        //"appDescription": null,
        //"developerName": null,
        //"developerUrl": null
      }
    }


    //"coast": false,
    //"coast": {
    //  "pictureAspect": "backgroundAndMargin",
    //  //"backgroundColor": "#FFFFFF",
    //  "margin": "0" // 12%
    //},

    //"openGraph": {
    //  "pictureAspect": "backgroundAndMargin",
    //  "backgroundColor": "#136497",
    //  "margin": "0", // 12%
    //  "ratio": "1.91:1"
    //},

    //"yandex": false
    //"yandexBrowser": {
    //  //"background_color": "#FFFFFF",
    //  "manifest": {
    //    "showTitle": true
    //    //"version": "1.0"
    //  }
    //}
  },
  "settings": {
    //"compression": "3",
    "scalingAlgorithm": "Mitchell",
    "errorOnImageTooSmall": true
  },
  //"filesLocation": {
  //  "type": "path",
  //  "path": ""
  //},
  "versioning": false,


  "appName": "@ViewBag.ApplicationName",
  "version": "1.0",
  "appDescription": null,
  "developerName": null,
  "developerUrl": null,
  "background": "#ffffff",
  "dir": "auto",
  "lang": "en-US",

  // Plugin settings
  //"apiKey": "eabf77c98d6bd1eea81fb58be7895c42dafc2b21",
  "online": false,
  "preferOnline": false,
  "inject": true,
  "applyCustomAttributes": false
