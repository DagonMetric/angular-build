    "unzip2": "^0.2.5",
    "update-notifier": "^2.5.0",
    "url-loader": "^1.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.14.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-merge": "^4.1.3",
