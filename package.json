{
  "name": "@bizappframework/angular-build",
  "version": "6.0.0",
  "description": "Build tool for Angular application and library projects.",
  "keywords": [
    "angular",
    "angular build tool",
    "angular cli",
    "angular cli builder",
    "angular devKit",
    "angular rollup",
    "angular webpack",
    "asp.net core angular build",
    "build tool",
    "favicons generator",
    "html inject"
  ],
  "author": "BizAppFramework Team",
  "license": "MIT",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizappframework/angular-build.git"
  },
  "homepage": "https://github.com/bizappframework/angular-build",
  "bin": {
    "ngb": "./bin/ngb"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "builders": "dist/builders.json",
  "scripts": {
    "clean": "rimraf -- dist schemas",
    "lint": "node node_modules/tslint/bin/tslint -t verbose -p tsconfig.json -c tslint.json",
    "tsc": "node node_modules/typescript/bin/tsc -p tsconfig.json",
    "generate-schemas": "node tools/generate-schemas.js",
    "copy-files": "node tools/copy-files.js",
    "ngb:version": "node dist/bin/ngb --version",
    "test": "echo \"Error: no test specified\" ",
    "build": "npm run clean && npm run lint && npm run tsc && npm run generate-schemas && npm run copy-files && npm run ngb:version",
    "build:samples": "npm run build && npm run build:sample-app && npm run build:sample-app-prod && npm run build:sample-lib",
    "build:sample-app": "node dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry",
    "build:sample-app-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry --env.prod --env.aot",
    "build:sample-lib": "node dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json  --disable-telemetry --env.prod"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.6.8",
    "@angular-devkit/build-optimizer": "^0.6.8",
    "@angular-devkit/core": "^0.6.8",
    "@angular/compiler": "^6.0.7",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/service-worker": "^6.0.7",
    "@ngtools/webpack": "^6.0.8",
    "ajv": "^6.5.2",
    "ajv-keywords": "^3.2.0",
    "applicationinsights": "^1.0.3",
    "autoprefixer": "^8.6.4",
    "builtins": "^2.0.0",
    "cache-loader": "^1.2.2",
    "clean-css": "^4.1.11",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.0-rc.2",
    "denodeify": "^1.2.1",
    "ejs": "^2.6.1",
    "favicons": "^5.1.1",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.2",
    "html-minifier": "^3.5.17",
    "license-webpack-plugin": "^1.3.1",
    "loader-utils": "^1.1.0",
    "magic-string": "^0.25.0",
    "mini-css-extract-plugin": "^0.4.1",
    "minimatch": "^3.0.4",
    "node-sass": "^4.9.0",
    "parse5": "^5.0.0",
    "postcss": "^6.0.23",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-url": "^7.3.2",
    "raw-loader": "^0.5.1",
    "resolve": "^1.8.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript2": "^0.15.1",
    "rxjs": "^6.2.1",
    "sass-loader": "^7.0.3",
    "semver": "^5.5.0",
    "sorcery": "^0.10.0",
    "source-map-url": "^0.4.0",
    "style-loader": "^0.21.0",
    "supports-color": "^5.4.0",
