{
  "name": "@dagonmetric/angular-build",
  "version": "8.0.2",
  "description": "Another build and packaging tool for Angular applications and library projects.",
  "keywords": [
    "angular-build",
    "angular",
    "cli",
    "angular-build-tools",
    "build-tools",
    "angular-library-builder",
    "angular-library-packaging-tools",
    "angular-architect-builder",
    "angular-devkit",
    "rollup",
    "webpack",
    "typescript",
    "html-injection"
  ],
  "author": "DagonMetric",
  "license": "MIT",
  "engines": {
    "node": ">=10.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DagonMetric/angular-build.git"
  },
  "bugs": {
    "url": "https://github.com/DagonMetric/angular-build/issues"
  },
  "homepage": "https://github.com/DagonMetric/angular-build",
  "bin": {
    "ngb": "./bin/ngb"
  },
  "experimental": true,
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "builders": "dist/src/architect/builders.json",
  "scripts": {
    "ng": "ng",
    "ngb": "ngb",
    "tsc": "tsc",
    "tslint": "tslint",
    "lint": "tslint -t verbose -p tsconfig.json -c tslint.json",
    "build": "rimraf -- dist && tsc && node tools/generate-schemas.js && node tools/copy-files.js && node dist/bin/ngb --version",
    "build:samples": "node ./dist/bin/ngb build --filter=lib1 --filter=lib1-secondary --filter=app1 --verbose && ng build lib1 --verbose -c=production && ng build lib1-secondary --verbose -c=production && ng build app1 --verbose -c=production",
    "build:lib1": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1 --verbose",
    "build:lib1:prod": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1 --verbose --env.prod",
    "build:lib1-secondary": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-secondary --verbose",
    "build:lib1-secondary:prod": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-secondary --verbose --env.prod",
    "build:lib1-project-extends": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-project-extends --verbose",
    "build:lib1-project-extends:prod": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-project-extends --verbose --env.prod",
    "build:lib1-builtin-extends": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-builtin-extends --verbose",
    "build:lib1-builtin-extends:prod": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-builtin-extends --verbose --env.prod",
    "build:lib1-file-extends": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-file-extends --verbose",
    "build:lib1-file-extends:prod": "node -r source-map-support/register ./dist/bin/ngb build --filter=lib1-file-extends --verbose --env.prod",
    "build:app1": "node -r source-map-support/register ./dist/bin/ngb build --filter=app1 --verbose",
    "build:app1:refdll": "node -r source-map-support/register ./dist/bin/ngb build --filter=app1 --verbose --env.refdll",
    "build:app1:prod": "node -r source-map-support/register ./dist/bin/ngb build --filter=app1 --verbose --env.prod",
    "build:app1-builtin-extends": "node -r source-map-support/register ./dist/bin/ngb build --filter=app1-builtin-extends --verbose",
    "build:ng:lib1": "ng build lib1 --verbose",
    "build:ng:lib1:prod": "ng build lib1 --verbose -c=production",
    "build:ng:lib1-secondary": "ng build lib1-secondary --verbose",
    "build:ng:lib1-secondary:prod": "ng build lib1-secondary --verbose -c=production",
    "build:ng:lib1-project-extends": "ng build lib1-project-extends --verbose",
    "build:ng:lib1-project-extends:prod": "ng build lib1-project-extends --verbose -c=production",
    "build:ng:lib1-builtin-extends": "ng build lib1-builtin-extends --verbose",
    "build:ng:lib1-builtin-extends:prod": "ng build lib1-builtin-extends --verbose -c=production",
    "build:ng:lib1-file-extends": "ng build lib1-file-extends --verbose",
    "build:ng:lib1-file-extends:prod": "ng build lib1-file-extends --verbose -c=production",
    "build:ng:app1": "ng build app1 --verbose",
    "build:ng:app1:prod": "ng build app1 --verbose -c=production"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.800.2",
    "@angular-devkit/build-optimizer": "^0.800.2",
    "@angular-devkit/core": "^8.0.2",
    "@ngtools/webpack": "^8.0.2",
    "ajv": "^6.10.0",
    "ajv-keywords": "^3.4.0",
    "autoprefixer": "^9.6.0",
    "builtins": "^2.0.1",
    "clean-css": "^4.2.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "denodeify": "^1.2.1",
    "ejs": "^2.6.1",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "html-minifier": "^4.0.0",
    "license-webpack-plugin": "^2.1.1",
    "loader-utils": "^1.2.3",
    "magic-string": "^0.25.2",
    "mini-css-extract-plugin": "^0.7.0",
    "minimatch": "^3.0.4",
    "node-sass": "^4.12.0",
    "parse5": "^5.1.0",
    "postcss": "^7.0.17",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "raw-loader": "~1.0.0",
    "resolve": "^1.11.1",
    "rollup": "^1.14.4",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rxjs": "~6.4.0",
    "sass-loader": "^7.1.0",
    "sorcery": "^0.10.0",
    "source-map-url": "^0.4.0",
    "style-loader": "^0.23.1",
    "supports-color": "^6.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "tsickle": "^0.35.0",
    "typescript": "~3.4.5",
    "uglify-js": "^3.6.0",
    "update-notifier": "^3.0.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "webpack-sources": "^1.3.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@angular/cli": "^8.0.2",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@types/autoprefixer": "^9.5.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/denodeify": "^1.2.31",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/html-minifier": "^3.5.3",
    "@types/loader-utils": "^1.1.3",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^12.0.7",
    "@types/node-sass": "^4.11.0",
    "@types/parse5": "^5.0.0",
    "@types/resolve": "^0.0.8",
    "@types/supports-color": "^5.3.0",
    "@types/uglify-js": "^3.0.4",
    "@types/webpack": "^4.4.32",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-sources": "^0.1.5",
    "@types/yargs": "^13.0.0",
    "rimraf": "^2.6.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.12",
    "tslint": "^5.17.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript-json-schema": "^0.38.3",
    "typescript-tslint-plugin": "^0.4.0",
    "webpack-dev-server": "^3.7.1",
    "zone.js": "^0.9.1"
  }
}
