{
  "name": "@bizappframework/angular-build",
  "version": "6.1.5",
  "description": "Build tool for Angular app and library projects based on Angular CLI.",
  "keywords": [
    "angular",
    "angular6",
    "angular build tool",
    "angular cli",
    "angular cli builder",
    "angular devKit",
    "angular rollup",
    "angular webpack",
    "angular asp.net core build",
    "build tool",
    "html inject",
    "Octalhouse"
  ],
  "author": "BizAppFramework Team",
  "license": "MIT",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizappframework/angular-build.git"
  },
  "homepage": "https://github.com/bizappframework/angular-build",
  "bin": {
    "ngb": "./bin/ngb"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "builders": "dist/builders.json",
  "scripts": {
    "clean": "rimraf -- dist schemas",
    "lint": "node node_modules/tslint/bin/tslint -t verbose -p tsconfig.json -c tslint.json",
    "tsc": "node node_modules/typescript/bin/tsc -p tsconfig.json",
    "generate-schemas": "node tools/generate-schemas.js",
    "copy-files": "node tools/copy-files.js",
    "ngb:version": "node dist/bin/ngb --version",
    "test": "npm run build:samples",
    "build": "npm run clean && npm run lint && npm run tsc && npm run generate-schemas && npm run copy-files && npm run ngb:version",
    "build:samples": "npm run build:sample-app-prod && npm run build:sample-lib-prod",
    "build:sample-app": "node -r source-map-support/register dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry",
    "build:sample-app-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry --env.prod --env.aot",
    "build:sample-lib": "node -r source-map-support/register dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json --verbose --disable-telemetry",
    "build:sample-lib-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json --env.prod --verbose --disable-telemetry"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.8.3",
    "@angular-devkit/build-optimizer": "^0.8.3",
    "@angular-devkit/core": "^0.8.3",
    "@angular/compiler": "^6.1.8",
    "@angular/compiler-cli": "^6.1.8",
    "@angular/core": "^6.1.8",
    "@ngtools/webpack": "^6.2.3",
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "applicationinsights": "^1.0.4",
    "autoprefixer": "^9.1.5",
    "builtins": "^2.0.0",
    "cache-loader": "^1.2.2",
    "clean-css": "^4.2.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "denodeify": "^1.2.1",
    "ejs": "^2.6.1",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "html-minifier": "^3.5.20",
    "license-webpack-plugin": "^2.0.1",
    "loader-utils": "^1.1.0",
    "magic-string": "^0.25.1",
    "mini-css-extract-plugin": "^0.4.3",
    "minimatch": "^3.0.4",
    "node-sass": "^4.9.3",
    "parse5": "^5.1.0",
    "postcss": "^7.0.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "raw-loader": "^0.5.1",
    "resolve": "^1.8.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "rxjs": "~6.2.2",
    "sass-loader": "^7.1.0",
    "semver": "^5.5.1",
    "sorcery": "^0.10.0",
    "source-map-url": "^0.4.0",
    "style-loader": "^0.23.0",
    "supports-color": "^5.5.0",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.2.0",
    "typescript": "~2.9.2",
    "uglify-js": "^3.4.9",
    "update-notifier": "^2.5.0",
    "url-loader": "^1.1.1",
    "uuid": "^3.3.2",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-merge": "^4.1.4",
    "webpack-sources": "^1.3.0",
    "yargs": "^12.0.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/common": "^6.1.8",
    "@angular/platform-browser": "^6.1.8",
    "@angular/platform-browser-dynamic": "^6.1.8",
    "@angular/router": "^6.1.8",
    "@types/autoprefixer": "^6.7.3",
    "@types/denodeify": "^1.2.31",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.0",
    "@types/html-minifier": "^3.5.2",
    "@types/loader-utils": "^1.1.3",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.10.3",
    "@types/node-sass": "^3.10.32",
    "@types/parse5": "^5.0.0",
    "@types/resolve": "0.0.8",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^5.5.0",
    "@types/supports-color": "^5.3.0",
    "@types/uglify-js": "^3.0.3",
    "@types/webpack": "^4.4.12",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-sources": "^0.1.5",
    "@types/yargs": "^12.0.0",
    "core-js": "^2.5.7",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.9",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript-json-schema": "^0.32.0"
  }
}
