{
  "name": "@dagonmetric/angular-build",
  "version": "8.0.0-beta.0",
  "description": "Another Angular build tool, and Angular Architect builder plugin for building Angular application and library projects.",
  "keywords": [
    "angular",
    "build tool",
    "angular build tool",
    "angular library build tool",
    "angular cli",
    "angular architect builder",
    "angular devKit",
    "rollup",
    "webpack",
    "typescript",
    "html injection",
    "dagonmetric"
  ],
  "author": "DagonMetric",
  "license": "MIT",
  "engines": {
    "node": ">=10.9.0 <11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DagonMetric/angular-build.git"
  },
  "bugs": {
    "url": "https://github.com/DagonMetric/angular-build/issues"
  },
  "homepage": "https://github.com/DagonMetric/angular-build",
  "bin": {
    "ngb": "./bin/ngb"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "builders": "dist/builders.json",
  "scripts": {
    "clean": "rimraf -- dist schemas",
    "lint": "node node_modules/tslint/bin/tslint -t verbose -p tsconfig.json -c tslint.json",
    "tsc": "node node_modules/typescript/bin/tsc -p tsconfig.json",
    "generate-schemas": "node tools/generate-schemas.js",
    "copy-files": "node tools/copy-files.js",
    "ngb:version": "node dist/bin/ngb --version",
    "test": "npm run build:samples",
    "build": "npm run clean && npm run tsc && npm run generate-schemas && npm run copy-files && npm run ngb:version",
    "build:samples": "npm run build:sample-app-prod && npm run build:sample-lib-prod",
    "build:sample-app": "node -r source-map-support/register dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry",
    "build:sample-app-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry --env.prod --env.aot",
    "build:sample-lib": "node -r source-map-support/register dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json --verbose --disable-telemetry",
    "build:sample-lib-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json --env.prod --verbose --disable-telemetry"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.14.0-beta.2",
    "@angular-devkit/build-optimizer": "^0.14.0-beta.2",
    "@angular-devkit/core": "^8.0.0-beta.2",
    "@ngtools/webpack": "^8.0.0-beta.2",
    "ajv": "^6.9.2",
    "ajv-keywords": "^3.4.0",
    "autoprefixer": "^9.4.9",
    "builtins": "^2.0.0",
    "clean-css": "^4.2.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "denodeify": "^1.2.1",
    "ejs": "^2.6.1",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "html-minifier": "^3.5.21",
    "license-webpack-plugin": "^2.1.1",
    "loader-utils": "^1.2.3",
    "magic-string": "^0.25.2",
    "mini-css-extract-plugin": "^0.5.0",
    "minimatch": "^3.0.4",
    "node-sass": "^4.11.0",
    "parse5": "^5.1.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "raw-loader": "^1.0.0",
    "resolve": "^1.10.0",
    "rollup": "^1.2.3",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rxjs": "~6.3.3",
    "sass-loader": "^7.1.0",
    "sorcery": "^0.10.0",
    "source-map-url": "^0.4.0",
    "style-loader": "^0.23.1",
    "supports-color": "^6.1.0",
    "terser-webpack-plugin": "^1.2.2",
    "ts-loader": "^5.3.3",
    "typescript": "3.2.4",
    "uglify-js": "^3.4.9",
    "update-notifier": "^2.5.0",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-sources": "^1.3.0",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@angular/common": "8.0.0-beta.6",
    "@angular/compiler": "8.0.0-beta.6",
    "@angular/compiler-cli": "8.0.0-beta.5",
    "@angular/core": "8.0.0-beta.6",
    "@angular/platform-browser": "8.0.0-beta.6",
    "@angular/platform-browser-dynamic": "8.0.0-beta.6",
    "@angular/router": "8.0.0-beta.6",
    "@types/autoprefixer": "^9.1.1",
    "@types/cross-spawn": "^6.0.0",
    "@types/denodeify": "^1.2.31",
    "@types/fs-extra": "^5.0.5",
    "@types/glob": "^7.1.1",
    "@types/html-minifier": "^3.5.3",
    "@types/loader-utils": "^1.1.3",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^11.9.5",
    "@types/node-sass": "^4.11.0",
    "@types/parse5": "^5.0.0",
    "@types/resolve": "0.0.8",
    "@types/supports-color": "^5.3.0",
    "@types/uglify-js": "^3.0.4",
    "@types/webpack": "^4.4.24",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-sources": "^0.1.5",
    "@types/yargs": "^12.0.9",
    "core-js": "^2.6.5",
    "rimraf": "^2.6.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.10",
    "tsickle": "^0.34.3",
    "tslint": "^5.13.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript-json-schema": "^0.36.0",
    "typescript-tslint-plugin": "^0.3.1",
    "zone.js": "^0.8.29"
  }
}
