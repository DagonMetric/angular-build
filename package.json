{
  "name": "@bizappframework/angular-build",
  "version": "6.0.0-beta.3",
  "description": "Build tool for Angular app and library projects.",
  "keywords": [
    "ngb",
    "cli",
    "ngb-cli",
    "angular",
    "build",
    "html inject",
    "angular cli alternative",
    "angular build tool",
    "angular app build tool",
    "angular lib build tool",
    "angular library build tool",
    "angular webpack",
    "angular rollup",
    "asp.net-core angular build tool",
    "asp.net-core html inject",
    "favicons app icons generator"
  ],
  "author": "BizAppFramework Team",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "ngb": "./bin/ngb"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizappframework/angular-build.git"
  },
  "homepage": "https://github.com/bizappframework/angular-build",
  "scripts": {
    "clean": "rimraf -- dist schemas",
    "lint": "node node_modules/tslint/bin/tslint -t verbose -p tsconfig.json -c tslint.json",
    "tsc": "node node_modules/typescript/bin/tsc -p tsconfig.json",
    "generate-schema": "node tools/generate-schema.js",
    "copy-files": "node tools/copy-files.js",
    "ngb:version": "node dist/bin/ngb --version",
    "test": "echo \"Error: no test specified\" ",
    "build": "npm run clean && npm run lint && npm run tsc && npm run generate-schema && npm run copy-files && npm run ngb:version",
    "build:test-all": "npm run build:test-app && npm run build:test-app-prod && npm run build:test-lib && npm run build:test-lib-prod",
    "build:test-app": "node dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry",
    "build:test-app-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-app-build-test/angular-build.json --disable-telemetry --env.prod --env.aot",
    "build:test-lib": "node dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json  --disable-telemetry",
    "build:test-lib-prod": "node dist/bin/ngb build --config=samples/hello-world-ng-lib-build-test/angular-build.json  --disable-telemetry --env.prod",
    "prerun:test-app": "npm run build:test-app",
    "run:test-app": "http-server ./samples/hello-world-ng-app-build-test/dist -p 3000 -o",
    "prerun:test-app-prod": "npm run build:test-app-prod",
    "run:test-app-prod": "http-server ./samples/hello-world-ng-app-build-test/dist -p 3000 -o"
  },
