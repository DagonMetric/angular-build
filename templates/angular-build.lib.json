{
  "$schema": "node_modules/@bizappframework/angular-build/schemas/schema-draft-04.json#",
  "apps": [],
  "libs": [
    {
      "srcDir": "src",
      "outDir": "dist",
      "assets": [
        "README.md",
        "LICENSE"
      ],
      "styles": [
        //{
        //  "from": "core/theming/prebuilt/styles.scss",
        //  "to": "prebuilt-themes/styles.css"
        //}
      ],
      // "entry": "index.ts",
      "tsconfig": "tsconfig.json",
      "tsTranspilations": [
        {
          //"name": "es2015-es2015",
          "tsconfig": "tsconfig.json"
          // default true
          //"copyTemplateAndStyleUrls": true
        }
      ],
      "bundleTargets": [
        {
          "name": "esm-es2015",
          "entryResolution": {
            "entryRoot": "tsTranspilationOutDir"
            // default to first item index
            //"tsTranspilationIndex": 0
          },
          "libraryTarget": "es",
          "outDir": "bundles"
        },
        {
          "name": "esm-es5",
          "entryResolution": {
            "entryRoot": "bundleTargetOutDir"
            // default to previous bundle target
            // "bundleTargetIndex": "esm-es2015"
          },
          "libraryTarget": "es",
          "outDir": "bundles",
          "addPureAnnotations": true,
          "transformScriptTargetOnly": true,
          "scriptTarget": "es5"
        },
        {
          "name": "umd-es5",
          "entryResolution": {
            "entryRoot": "bundleTargetOutDir"
            // default to previous bundle target
            // "bundleTargetIndex": "esm-es5"
          },
          "libraryTarget": "umd",
          "outDir": "bundles",
          "inlineResources": false
        }
      ],
      "packageOptions": {
        "packageConfigFile": "package.json"
      },
      "banner": "banner.txt",
      "envOverrides": {
        "dev": {

        },
        "prod": {

        }
      }
    }
  ]
}