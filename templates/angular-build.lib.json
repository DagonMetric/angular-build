{
  "$schema": "node_modules/@bizappframework/angular-build/schemas/schema-draft-04.json#",
  "apps": [],
  "libs": [
    {
      "srcDir": "src",
      "outDir": "dist",
      "styles": [
        {
          "from": "",
          "to": ""
        }
      ],
      "entry": "index.js",
      "tsconfig": "tsconfig.json",
      "tsTranspilations": [
        {
          "tsconfig": "tsconfig.json",
          "copyTemplateAndStyleUrls": true
        }
      ],
      "bundleTargets": [
        {
          "name": "esm-es2015",
          "entryResolution": {
            "entryRoot": "tsTranspilationOutDir"
          },
          "libraryTarget": "es",
          "outDir": "bundles"
        },
        {
          "name": "esm-es5",
          "entryResolution": {
            "entryRoot": "bundleTargetOutDir",
            "bundleTargetIndex": "esm-es2015"
          },
          "outDir": "bundles",
          "transformScriptTargetOnly": true,
          "scriptTarget": "es5"
        },
        {
          "name": "umd-es5",
          "entryResolution": {
            "entryRoot": "bundleTargetOutDir"
          },
          "libraryTarget": "umd",
          "outDir": "bundles"
        }
      ],
      "packageOptions": {
        "packageConfigFile": "package.json",
        "main": "",
        "module": "",
        "es2015": "",
        "typings": ""
      },
      "envOverrides": {
        "dev": {

        },
        "prod": {
          
        } 
      }
    }
  ]
}