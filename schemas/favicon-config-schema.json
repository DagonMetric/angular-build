                        },
                        "pictureAspect": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "ios": {
                    "properties": {
                        "assets": {
                            "properties": {
                                "declareOnlyDefaultIcon": {
                                    "type": "boolean"
                                },
                                "ios6AndPriorIcons": {
                                    "type": "boolean"
                                },
                                "ios7AndLaterIcons": {
                                    "type": "boolean"
                                },
                                "precomposedIcons": {
                                    "type": "boolean"
                                }
                            },
                            "type": "object"
                        },
                        "backgroundColor": {
                            "type": "string"
                        },
                        "margin": {
                            "type": [
                                "string",
                                "number"
                            ]
                        },
                        "pictureAspect": {
                            "type": "string"
                        },
                        "startupImage": {
                            "properties": {
                                "backgroundColor": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "type": "object"
                },
                "openGraph": {
                    "properties": {
                        "backgroundColor": {
                            "type": "string"
                        },
                        "margin": {
                            "type": [
                                "string",
                                "number"
                            ]
                        },
                        "pictureAspect": {
                            "type": "string"
                        },
                        "ratio": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "safariPinnedTab": {
                    "properties": {
                        "pictureAspect": {
                            "type": "string"
