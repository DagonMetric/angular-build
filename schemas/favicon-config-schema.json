                        },
                        "threshold": {
                            "type": "number"
                        }
                    },
                    "type": "object"
                },
                "windows": {
                    "properties": {
                        "assets": {
                            "properties": {
                                "windows10Ie11EdgeTiles": {
                                    "properties": {
                                        "big": {
                                            "type": "boolean"
                                        },
                                        "medium": {
                                            "type": "boolean"
