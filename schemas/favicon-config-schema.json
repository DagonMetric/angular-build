            "type": "object"
        }
    },
    "properties": {
        "apiKey": {
            "type": "string"
        },
        "appDescription": {
            "type": "string"
        },
        "appName": {
            "description": "see https://github.com/haydenbleasel/favicons#usage",
            "type": "string"
        },
        "background": {
            "description": "see https://github.com/haydenbleasel/favicons#usage",
            "type": "string"
        },
        "design": {
            "$ref": "#/definitions/IconDesign",
            "description": "see https://github.com/haydenbleasel/favicons#usage"
        },
        "developerName": {
            "type": "string"
        },
        "developerUrl": {
            "type": "string"
        },
        "dir": {
            "type": "string"
        },
        "display": {
            "description": "Android display: \"browser\" or \"standalone\".",
            "type": "string"
        },
        "emitFaviconIcoToOutDirRoot": {
            "description": "Copy 'favicon.ico' file to root output directory",
            "type": "boolean"
        },
        "emitStats": {
            "default": false,
            "description": "Emit all stats of the generated icons",
            "type": "boolean"
        },
        "iconsPath": {
            "description": "The prefix for all image files (might be a folder or a name)",
            "type": "string"
        },
        "lang": {
            "type": "string"
        },
        "masterPicture": {
            "description": "Your source logo",
            "type": "string"
        },
        "online": {
            "description": "Use RealFaviconGenerator to create favicons? `boolean`",
            "type": "boolean"
        },
        "orientation": {
            "description": "Android orientation: \"portrait\" or \"landscape\".",
            "type": "string"
        },
        "persistentCache": {
            "type": "boolean"
        },
        "preferOnline": {
            "description": "Use offline generation, if online generation has failed. `boolean`",
            "type": "boolean"
        },
        "settings": {
            "properties": {
                "compression": {
                    "type": "string"
                },
                "errorOnImageTooSmall": {
                    "type": "boolean"
                },
                "scalingAlgorithm": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "startUrl": {
            "description": "Android start application's Url.",
            "type": "string"
        },
        "statsFilename": {
            "description": "The name of the json containing all favicon information",
            "type": "string"
        },
        "version": {
            "description": "Your application's version number.",
            "type": "string"
        },
        "versioning": {
            "anyOf": [
                {
                    "properties": {
                        "paramName": {
                            "type": "string"
                        },
                        "paramValue": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                {
                    "type": "boolean"
                }
            ]
        }
    },
    "type": "object"
}