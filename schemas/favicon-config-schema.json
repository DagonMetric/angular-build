                        },
                        "manifest": {
                            "properties": {
                                "declared": {
                                    "type": "boolean"
                                },
                                "display": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "onConflict": {
                                    "type": "string"
                                },
                                "orientation": {
                                    "type": "string"
                                },
                                "startUrl": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "pictureAspect": {
