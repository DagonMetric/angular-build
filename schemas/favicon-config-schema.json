                        "circleInnerMargin": {
                            "type": "string"
                        },
                        "keepPictureInCircle": {
                            "type": [
                                "string",
                                "boolean"
                            ]
                        },
                        "manifest": {
                            "properties": {
                                "appDescription": {
                                    "type": "string"
                                },
                                "appName": {
                                    "type": "string"
                                },
                                "developerName": {
                                    "type": "string"
                                },
                                "developerUrl": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "margin": {
                            "type": [
                                "string",
