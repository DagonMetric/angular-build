{
  "$schema": "../dist/src/schemas/app-project-config-schema.json#",
  "platformTarget": "web",
  "root": "src",
  "outputPath": "dist",
  "entry": "main.ts",
  "polyfills": [
    "polyfills.ts"
  ],
  "tsConfig": "tsconfig.app.json",
  "copy": [
    "assets/**/*",
    "favicon.ico"
  ],
  "styles": [
    "styles.css"
  ],
  "publicPath": "/",
  "baseHref": "/",
  "htmlInject": {
    "resourceHints": true,
    "index": "index.html"
  },
  "envOverrides": {
    "prod": {
      "optimization": true,
      "aot": true,
      "extractCss": true,
      "buildOptimizer": true,
      "sourceMap": false,
      "namedChunks": false,
      "vendorChunk": false,
      "outputHashing": {
        "bundles": true,
        "chunks": true,
        "extractedAssets": true
      },
      "fileReplacements": [{
        "replace": "./environments/environment.ts",
        "with": "./environments/environment.prod.ts"
      }]
    }
  }
}
