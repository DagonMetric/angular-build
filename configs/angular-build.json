{
  "$schema": "node_modules/@bizappframework/angular-build/configs/schema.json#",
  "apps": [
    {
      "name": "browser-app",
      "target": "web",
      "root": "",
      "outDir": "wwwroot",
      "main": "",
      "test": "karma-test.browser.ts",
      "assets": [
        "assets"
      ],
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "polyfills": [
        "polyfills.browser.ts"
      ],
      "dlls": [
        //{
        //  "entry": "rxjs.imports.ts",
        //  "importToMain": true
        //},
        {
          "entry": "../package.json",
          "excludes": [
            "@angular/platform-server",
            "angular2-platform-node",
            "@angular/service-worker",
            "angular2-universal-polyfills",
            "aspnet-prerendering",
            "core-js",
            "es6-promise",
            "es6-shim",
            "event-source-polyfill",
            "ie-shim",
            "isomorphic-fetch",
            "preboot",
            "reflect-metadata",
            "rxjs",
            "tslib",
            "ts-helpers",
            "zone.js"
          ]
        }
      ],
      // TODO: to check
      "tsconfig": "../tsconfig.app.json",
      "faviconConfig": "favicon-config.json",
      "publicPath": "/",
      "index": "",
      "htmlInjectOptions": {
        "scriptsOutFileName": "",
        "iconsOutFileName": "",
        "stylesOutFileName": "",
        "customTagAttributes": []
      },
      "environmentSource": "environments/environment.ts",
      "buildTargetOverrides": {
        "dll": {},
        "dev": {
          // Default - true
          "referenceDll": true,
          "polyfills": [
            "polyfills.browser.ts",
            "event-source-polyfill"
          ]
        },
        "prod": {
          "environmentFile": "environments/environment.prod.ts",
          // Default - false
          "sourceMap": false,
          "moduleReplacements": [
            {
              "resourceRegExp": "zone\\.js(\\\\|\\/)dist(\\\\|\\/)long-stack-trace-zone",
              // Path is relative to project root dir, Default - angular-build's empty.js module
              "newResource": "empty.js"
            },
            {
              "resourceRegExp": "@angular(\\|/)compiler",
              "newResource": "empty.js"
            },
            {
              "resourceRegExp": "angular2-hmr",
              "newResource": "empty.js"
            }
          ]
        },
        "aot": {
          //"main": "main.browser.aot.ts",
          //"tsconfig": "../tsconfig.app.aot.json"
        },
        "universal": {},
        "test": {
          "inlineChunk": false,
          "vendorChunk": false,
          "dlls": [],
          "polyfills": [],
          "tsconfig": "../tsconfig.test.json"
        }
      }
    },
    {
      "name": "node-app",
      "extends": "browser-app",
      "target": "node",
      "outDir": "dist",
      "main": "",
      "test": "",
      "inlineChunk": false,
      "styles": [],
      "polyfills": [],
      "dlls": [
        {
          "entry": "../package.json",
          "excludes": [
            "@angular/service-worker",
            "core-js",
            "event-source-polyfill",
            "ie-shim",
            "isomorphic-fetch",
            "preboot",
            "tslib",
            "ts-helpers"
          ]
        }
      ],
      "index": "",
      "htmlInjectOptions": {},
      "skipGenerateIcons": true,
      "extractCss": false,
      "buildTargetOverrides": {
        "dll": {
          //"skip": false
        },
        "dev": {
          "referenceDll": true
        },
        "prod": {
          "environmentFile": "environments/environment.prod.ts",
          "sourceMap": false,
          "extractCss": false,
          "moduleReplacements": [
          ]
        },
        "universal": {
          "skip": false
        }
      },
      "skip": true
    }
  ],
  "test": {
    "codeCoverage": {
      "exclude": []
      //"disabled": true
    }
  },
  "buildOptions": {
    "typescriptWebpackTool": "@ngtools/webpack"
  }
}