{
  "apps": [
    {
      "name": "client",
      "target": "web",
      "root": "",
      "outDir": "",
      "main": "",
      "assets": [],
      "styles": [],
      "scripts": [],
      "polyfills": [ "polyfills.browser.ts" ],
      "dlls": [
        {
          "entry": "rxjs.imports.ts",
          "importToMain": true
        },
        {
          "entry": "../package.json",
          "excludes": [
            "@angular/compiler-cli",
            "@angular/platform-server",
            "angular2-platform-node",
            "angular2-universal-polyfills",
            "aspnet-prerendering",
            "core-js",
            "es6-promise",
            "es6-shim",
            "ie-shim",
            "isomorphic-fetch",
            "reflect-metadata",
            "rxjs",
            "tslib",
            "ts-helpers",
            "zone.js"
          ]
        }
      ],
      "tsconfig": "../tsconfig.webpack.json",
      "faviconConfig": "favicon-config.json",
      "publicPath": "/",
      "index": "",
      "htmlInjectOptions": {
        "indexOutFileName": "",
        "iconsOutFileName": "",
        "stylesOutFileName": "",
        "customTagAttributes": []
      },
      "environmentSource": "environments/environment.ts",
      "buildTargetOverrides": {
        "dev": {
          // Default - true
          "referenceDll": true
          //"skipGenerateIcons": true
        },
        "prod": {
          "environmentFile": "environments/environment.prod.ts",
          // Default - true
          "compressAssets": true,
          // Default - false
          "sourceMap": false,
          "moduleReplacements": [
            {
              "resourceRegExp": "zone\\.js(\\\\|\\/)dist(\\\\|\\/)long-stack-trace-zone",
              // Path is relative to project root dir, Default - angular-build's empty.js module
              "newResource": "empty.js"
            },
            {
              "resourceRegExp": "@angular(\\|/)compiler",
              // Path is relative to project root dir, Default - angular-build's empty.js module
              "newResource": "empty.js"
            },
            {
              "resourceRegExp": "angular2-hmr",
              // Path is relative to project root dir, Default - angular-build's empty.js module
              "newResource": "empty.js"
            }
          ]
        },
        "aot": {
          //"main": "main.browser.aot.ts",
          "tsconfig": "../tsconfig.webpack.aot.json"
        },
        "universal": {
        }
      }
    }
  ]
}